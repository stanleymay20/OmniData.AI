# Environment
ENVIRONMENT=development
API_HOST=0.0.0.0
API_PORT=8000

# Database Configuration
DB_HOST=db
DB_PORT=5432
DB_NAME=omnidata
DB_USER=postgres
DB_PASSWORD=postgres
DATABASE_URL=postgresql://postgres:postgres@db:5432/omnidata

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Authentication
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Payment Processing
STRIPE_SECRET_KEY=your_stripe_secret_key_here
STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key_here
PAYPAL_CLIENT_ID=your_paypal_client_id_here
PAYPAL_SECRET=your_paypal_secret_here

# Service URLs
BACKEND_URL=http://backend:8000
FRONTEND_URL=http://frontend:8501
MLFLOW_TRACKING_URI=http://mlflow:5000

# Security
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
ENCRYPTION_KEY=your_32_byte_encryption_key_here
API_TOKEN=your_api_token_here
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
SECURITY_ALERT_THRESHOLD=100
KEY_ROTATION_INTERVAL_DAYS=30

# Redis
REDIS_HOST=redis
REDIS_PORT=6379

# Airflow
AIRFLOW_SECRET_KEY=your_airflow_secret_key_here

# Monitoring
SENTRY_DSN=your_sentry_dsn_here

# Storage
DATA_STORAGE_PATH=/app/data
MODEL_STORAGE_PATH=/app/models
TEMP_STORAGE_PATH=/app/temp

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s

# Messaging
TELEGRAM_API_URL=https://api.telegram.org/bot${TELEGRAM_BOT_TOKEN}/sendMessage

# Development Tools
NGROK_TOKEN=your_ngrok_token_here
BUFFER_RECOVERY_CODE=your_buffer_recovery_code_here
GITHUB_TOKEN=your_github_token_here

# Firebase Config
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_AUTH_DOMAIN=your_firebase_auth_domain_here
FIREBASE_PROJECT_ID=your_firebase_project_id_here
FIREBASE_STORAGE_BUCKET=your_firebase_storage_bucket_here
FIREBASE_MESSAGING_SENDER_ID=your_firebase_messaging_sender_id_here
FIREBASE_APP_ID=your_firebase_app_id_here 